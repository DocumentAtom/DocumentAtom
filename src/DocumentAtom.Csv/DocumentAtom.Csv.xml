            <summary>
            CSV processor settings.
            </summary>
        </member>
        <member name="M:DocumentAtom.Csv.CsvProcessor.#ctor(DocumentAtom.Csv.CsvProcessorSettings)">
            <summary>
            Create atoms from CSV documents.
            </summary>
        </member>
        <member name="M:DocumentAtom.Csv.CsvProcessor.Dispose(System.Boolean)">
            <summary>
            Dispose.
            </summary>
            <param name="disposing">Disposing.</param>
        </member>
        <member name="M:DocumentAtom.Csv.CsvProcessor.Dispose">
            <summary>
            Dispose.
            </summary>
        </member>
        <member name="M:DocumentAtom.Csv.CsvProcessor.Extract(System.String)">
            <summary>
            Extract atoms from a file.
            </summary>
            <param name="filename">Filename.</param>
            <returns>Atoms.</returns>
        </member>
        <member name="T:DocumentAtom.Csv.CsvProcessorSettings">
            <summary>
            Settings for CSV processor.
            </summary>
        </member>
        <member name="P:DocumentAtom.Csv.CsvProcessorSettings.RowDelimiter">
            <summary>
            Delimiter found between rows.
            Default is Environment.NewLine.
            </summary>
        </member>
        <member name="P:DocumentAtom.Csv.CsvProcessorSettings.ColumnDelimiter">
            <summary>
            Delimiter found between columns within a row.
            Default is comma (,).
            </summary>
        </member>
        <member name="P:DocumentAtom.Csv.CsvProcessorSettings.QuoteCharacters">
            <summary>
            Characters to strip if found at the beginning or end of a value.
            Default is double quote and single quote.
            </summary>
        </member>
        <member name="P:DocumentAtom.Csv.CsvProcessorSettings.HasHeaderRow">
            <summary>
            Indicates whether the first row contains header names.
            Default is true.
            </summary>
        </member>
        <member name="P:DocumentAtom.Csv.CsvProcessorSettings.RowsPerAtom">
            <summary>
            Number of rows per atom.
            0 = entire table as one atom (default).
            1 = each row as separate atom.
            N = N rows per atom.
            Minimum value is 0.
            </summary>
        </member>
        <member name="M:DocumentAtom.Csv.CsvProcessorSettings.#ctor">
            <summary>
            Settings for CSV processor.
            </summary>
        </member>
    </members>
